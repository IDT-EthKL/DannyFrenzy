        "express": "^4.21.0",
        "grammy": "^1.30.0",
        "phaser": "^3.85.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "thirdweb": "^5.61.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
